{
  "name": "comments-benchmark",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "wrangler pages dev .",
    "serve:benchmark": "serve -p 8788 ./benchmark-output",
    "serve:static": "serve -p 8788 ./dist",
    "autocannon": "node ./run-autocannon.js",
    "autocannon:islands": "autocannon -d=30 https://comments-benchmark.pages.dev/islands/10",
    "autocannon:local:islands": "autocannon -d=30 http://127.0.0.1:8788/islands/10",
    "print:content-desktop-table": "FORM_FACTOR=desktop npm run print -- content-table",
    "print:content-mobile-table": "FORM_FACTOR=mobile npm run print -- content-table",
    "print:comments-box-plot": "npm run print -- comments-box-plot",
    "print:comments-sizes": "npm run print -- comments-sizes",
    "print:comments-desktop-table": "FORM_FACTOR=desktop npm run print -- comments-table",
    "print:comments-mobile-table": "FORM_FACTOR=mobile npm run print -- comments-table",
    "print:ecommerce-box-plot": "npm run print -- ecommerce-box-plot",
    "print:ecommerce-sizes": "npm run print -- ecommerce-sizes",
    "print:ecommerce-desktop-table": "FORM_FACTOR=desktop npm run print -- ecommerce-table",
    "print:ecommerce-mobile-table": "FORM_FACTOR=mobile npm run print -- ecommerce-table",
    "print:social-desktop-table": "FORM_FACTOR=desktop npm run print -- social-table",
    "print:social-mobile-table": "FORM_FACTOR=mobile npm run print -- social-table",
    "print": "deno run -A ./scripts/print.ts",
    "lighthouse:blog:desktop": "FORM_FACTOR=desktop playwright test tests/blog.spec.ts",
    "lighthouse:blog:mobile": "FORM_FACTOR=mobile playwright test tests/blog.spec.ts",
    "lighthouse:content:desktop": "FORM_FACTOR=desktop playwright test tests/content.spec.ts",
    "lighthouse:content:mobile": "FORM_FACTOR=mobile playwright test tests/content.spec.ts",
    "lighthouse:ecommerce:desktop": "FORM_FACTOR=desktop node --loader ts-node/esm ./tests/ecommerce.spec.ts",
    "lighthouse:ecommerce:mobile": "FORM_FACTOR=mobile node --loader ts-node/esm ./tests/ecommerce.spec.ts",
    "lighthouse:social:desktop": "FORM_FACTOR=desktop playwright test tests/social.spec.ts",
    "lighthouse:social:mobile": "FORM_FACTOR=mobile playwright test tests/social.spec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nanoid": "^5.0.7",
    "rmfr": "^2.0.0",
    "wrangler": "^3.80.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.7.5",
    "@types/rmfr": "^2.0.5",
    "autocannon": "^7.15.0",
    "glob": "^11.0.0",
    "lighthouse": "^12.2.1",
    "mkdirp": "^3.0.1",
    "playwright": "^1.48.0",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "^3.3.3",
    "puppeteer": "^23.5.3",
    "serve": "^14.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
